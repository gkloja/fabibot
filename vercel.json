{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/index.js"
    },
    {
      "src": "/series/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/movie/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)\\.ts",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}